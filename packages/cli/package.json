{
  "name": "@koushik_xd/dash",
  "version": "1.2.0",
  "description": "AI-powered git commit message generator",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Koushik"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koushikxd/dash-cli.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/koushikxd/dash-cli/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dash": "dist/index.js"
  },
  "scripts": {
    "build": "bunup --clean",
    "dev": "bunup --watch",
    "test": "bunup --clean && vitest run",
    "lint": "biome check .",
    "clean": "git clean -xdf .cache .turbo node_modules dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "git",
    "commit",
    "ai",
    "groq",
    "cli",
    "conventional-commits"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/ini": "^4.1.1",
    "@types/node": "^25.0.2",
    "bunup": "^0.16.10",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "cleye": "^1.3.4",
    "execa": "^9.6.0",
    "groq-sdk": "^0.32.0",
    "ini": "^5.0.0",
    "kolorist": "^1.8.0"
  }
}
